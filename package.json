{
  "name": "video-viewer",
  "version": "1.0.0",
  "description": "A web application to display and preview videos in a folder",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "package": "pkg . --targets node18-win-x64 --output video-viewer"
  },
  "pkg": {
    "assets": [
      "public/**/*",
      "video/**/*"
    ]
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "author": "",
  "license": "ISC"
}